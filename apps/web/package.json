{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "echo": "envcli echo $:{NEXT_PUBLIC_API_URL}$:{API_PING_PATH}",
    "init": "node init.js",
    "pretify": "npx prettier . --write",
    "dev": "npm run openapi && envcli wait-port $:{NEXT_PUBLIC_API_URL}$:{API_PING_PATH} && concurrently --kill-others-on-fail -n next,routing -c green,magenta \"envcli next dev -p $:{NEXT_PUBLIC_APP_PORT}\" \"npm run dr:build:watch\"",
    "build": "npm run dr:build && npm run openapi && envcli wait-port $:{NEXT_PUBLIC_API_URL}$:{API_PING_PATH} && next build",
    "build:no:check": "npm run dr:build && npm run openapi && envcli wait-port $:{NEXT_PUBLIC_API_URL}$:{API_PING_PATH} && cross-env NO_CHECK=true next build",
    "start": "envcli wait-port $:{NEXT_PUBLIC_API_URL}$:{API_PING_PATH} && envcli next start -p $:{NEXT_PUBLIC_APP_PORT}",
    "start:local": "envcli wait-port $:{NEXT_PUBLIC_API_URL}$:{API_PING_PATH} && envcli next start -p $:{NEXT_PUBLIC_APP_PORT}",
    "lint": "next lint",
    "test": "echo 'all app:web tests passed!'",
    "clean": "rimraf .next",
    "dr:build": "npx --yes declarative-routing build",
    "dr:build:watch": "npx --yes declarative-routing build --watch",
    "openapi": "npm run openapi:yaml && npm run openapi:html",
    "openapi:yaml": "tsx ./src/routes/openapi.ts",
    "openapi:html": "npx --yes @redocly/cli build-docs openapi-docs.yml"
  },
  "dependencies": {
    "@million/lint": "^1.0.8",
    "@repo/directus-sdk": "*",
    "@repo/ui": "*",
    "@tanstack/react-query": "^5.40.1",
    "@tanstack/react-query-devtools": "^5.40.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "declarative-routing": "^0.1.19",
    "lucide-react": "^0.292.0",
    "next": "15.0.1",
    "next-auth": "^5.0.0-beta.22",
    "nextjs-toploader": "^1.6.12",
    "ofetch": "^1.3.4",
    "qs": "^6.12.1",
    "query-string": "^9.0.0",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "rimraf": "^6.0.1",
    "sass": "^1.69.5",
    "wait-port": "^1.1.0",
    "xior": "^0.5.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^7.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@n0safe/envcli": "^1.0.2",
    "@next/eslint-plugin-next": "15.0.1",
    "@repo/eslint-config": "*",
    "@repo/prettier-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/tsconfig": "*",
    "@tanstack/eslint-plugin-query": "^5.43.1",
    "@types/node": "^20.9.0",
    "@types/qs": "^6.9.15",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "autoprefixer": "^10.4.16",
    "node-env-run": "^4.0.2",
    "postcss": "^8.4.31",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "yaml": "^2.4.5"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
