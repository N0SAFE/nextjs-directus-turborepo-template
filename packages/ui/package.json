{
    "name": "@repo/ui",
    "version": "1.0.0",
    "sideEffects": [
        "**/*.css"
    ],
    "main": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "./globals.css": "./dist/index.css",
        "./*": {
            "import": {
                "types": "./dist/*.d.ts",
                "default": "./dist/*.mjs"
            },
            "require": {
                "types": "./dist/*.d.ts",
                "default": "./dist/*.js"
            }
        }
    },
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist",
        "pretify": "npx prettier ./src/**/*.{ts,tsx,js,jsx,css} --write",
        "init": "npm run build",
        "dev": "concurrently -c \"blue,green\" -n \"files,types\" \"tsup --watch\" \"tsc --build --force --pretty --preserveWatchOutput --verbose --diagnostics --watch\"",
        "build": "concurrently -c \"blue,green\" -n \"files,types\" \"tsup\" \"tsc --build --force --pretty --preserveWatchOutput --verbose --diagnostics\"",
        "lint": "eslint \"**/*.ts*\"",
        "test": "echo 'all packages:ui tests passed!'",
        "ui:add": "npx shadcn-ui@latest add"
    },
    "peerDependencies": {
        "react": "^18.2.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.7.0",
        "@repo/eslint-config": "*",
        "@repo/prettier-config": "*",
        "@repo/tailwind-config": "*",
        "@repo/tsconfig": "*",
        "@types/node": "^20.9.0",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "autoprefixer": "^10.4.16",
        "delete-cli": "^0.1.3",
        "next": "^14.0.2",
        "postcss": "^8.4.31",
        "prettier-plugin-tailwindcss": "^0.6.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwind-variants": "^0.1.18",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.6.0",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-separator": "^1.0.3",
        "@radix-ui/react-slot": "^1.0.2",
        "@repo/tsup-config": "*",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "concurrently": "^8.2.2",
        "framer-motion": "^10.16.4",
        "lucide-react": "^0.292.0",
        "next-themes": "^0.3.0",
        "react-hook-form": "^7.51.5",
        "rimraf": "^6.0.0",
        "tailwind-merge": "^2.0.0",
        "tailwindcss-animate": "^1.0.7",
        "tsup": "^6.2.0",
        "zod": "^3.23.8"
    }
}
